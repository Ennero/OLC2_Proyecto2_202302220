public static void main(String[] args) {
    System.out.println("--- Prueba de Unicode en char ---");
    char r = '\u0082';       // decimal 82 -> 'R'
    char currency = '\u00164'; // decimal 164 -> '¤'

    System.out.println(r);
    System.out.println(currency);

    System.out.println("R esperado: " + r);
    System.out.println("¤ esperado: " + currency);

    // También via String.valueOf
    System.out.println("via valueOf R: " + String.valueOf(r));
    System.out.println("via valueOf ¤: " + String.valueOf(currency));
}
