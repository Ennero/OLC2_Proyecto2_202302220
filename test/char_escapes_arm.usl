// Test de char con escapes, incluyendo unicode decimal
int main() {
  char a = '\n';
  char b = '\''; // comilla simple
  char c = '\\'; // backslash
  char d = 'A';
  char e = '\u65'; // 'A' en decimal (65)
  char f = '\u010'; // 8 octal-like? should parse as decimal 10 per our rule
  char g = '\u00010'; // decimal 10, con ceros
  System.out.println(a); // imprime salto de l√≠nea
  System.out.println(b);
  System.out.println(c);
  System.out.println(d);
  System.out.println(e);
  System.out.println(f);
  System.out.println(g);
  return 0;
}
