{
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: gcc compilar archivo activo",
			"command": "/usr/bin/gcc",
			"args": [
				"-fdiagnostics-color=always",
				"-g",
				"${file}",
				"-o",
				"${fileDirname}/${fileBasenameNoExtension}"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "Tarea generada por el depurador."
		},
		{
			"label": "build-project",
			"type": "shell",
			"command": "make",
			"group": "build"
		},
		{
			"label": "rebuild-project",
			"type": "shell",
			"command": "make",
			"group": "build"
		},
		{
			"label": "build-after-codegen-update",
			"type": "shell",
			"command": "make",
			"group": "build"
		},
		{
			"label": "rebuild-after-stringy-fix",
			"type": "shell",
			"command": "make",
			"group": "build"
		},
		{
			"label": "rebuild-after-ops-codegen",
			"type": "shell",
			"command": "make",
			"group": "build"
		},
		{
			"label": "arm64 test equals+casts",
			"type": "shell",
			"command": "./build/javalang --arm test/pruebaReferencia.usl && aarch64-linux-gnu-gcc -no-pie -static -s -o arm/salida arm/salida.s && qemu-aarch64 ./arm/salida",
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		},
		{
			"label": "arm64 run equals_casts",
			"type": "shell",
			"command": "./build/javalang --arm test/equals_casts.usl && aarch64-linux-gnu-gcc -no-pie -static -s -o arm/salida arm/salida.s && qemu-aarch64 ./arm/salida",
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		},
		{
			"label": "arm64 run equals_casts (retry)",
			"type": "shell",
			"command": "./build/javalang --arm test/equals_casts.usl && aarch64-linux-gnu-gcc -no-pie -static -s -o arm/salida arm/salida.s && qemu-aarch64 ./arm/salida",
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		},
		{
			"label": "arm64 run equals_casts (content eq)",
			"type": "shell",
			"command": "./build/javalang --arm test/equals_casts.usl && aarch64-linux-gnu-gcc -no-pie -static -s -o arm/salida arm/salida.s && qemu-aarch64 ./arm/salida",
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		},
		{
			"label": "arm64 run equals_casts (final)",
			"type": "shell",
			"command": "./build/javalang --arm test/equals_casts.usl && aarch64-linux-gnu-gcc -no-pie -static -s -o arm/salida arm/salida.s && qemu-aarch64 ./arm/salida",
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		},
		{
			"label": "build run switch_basico",
			"type": "shell",
			"command": "./build/javalang --arm test/switch_basico.usl && ./ensamblar.sh && qemu-aarch64 ./arm/salida",
			"problemMatcher": [
				"$gcc"
			]
		},
		{
			"label": "build-project",
			"type": "shell",
			"command": "make -j8",
			"isBackground": false,
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		},
		{
			"label": "build-project",
			"type": "shell",
			"command": "make",
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		}
	],
	"version": "2.0.0"
